{
	"expo": {
		"name": "recipetohome",
		"slug": "recipetohome",
		"version": "2.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"jsEngine": "hermes",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.rupakhetibinit.recipetohome"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"googleServicesFile": "./google-services.json",
			"package": "com.rupakhetibinit.recipetohome"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}

